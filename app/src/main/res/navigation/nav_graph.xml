<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/onboardingFragment">
    <fragment
        android:id="@+id/userAuthenticationFragment"
        android:name="de.gematik.ti.erp.app.userauthentication.ui.UserAuthenticationFragment"
        android:label="UserAuthenticationFragment" />
    <fragment
        android:id="@+id/debugFragment"
        android:name="de.gematik.ti.erp.app.debug.ui.DebugFragment"
        android:label="fragment_debug" />
    <fragment
        android:id="@+id/mainScreenFragment"
        android:name="de.gematik.ti.erp.app.mainscreen.ui.MainScreenFragment"
        android:label="MainScreenFragment">
        <action
            android:id="@+id/action_mainScreenFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim">
        </action>
        <action
            android:id="@+id/action_mainScreenFragment_to_prescriptionDetailFragment"
            app:destination="@id/prescriptionDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_mainScreenFragment_to_cardWallFragment"
            app:destination="@id/cardWallFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_mainScreenFragment_to_pharmacySearchFragment"
            app:destination="@id/pharmacySearchFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_mainScreenFragment_to_scanPrescriptionFragment"
            app:destination="@id/scanPrescriptionFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_mainScreenFragment_to_pickupCodeFragment"
            app:destination="@id/pickupCodeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/dataTermsFragment"
        android:name="de.gematik.ti.erp.app.terms.DataTermsFragment"
        android:label="DataTermsFragment" />
    <fragment
        android:id="@+id/termsOfUseFragment"
        android:name="de.gematik.ti.erp.app.terms.TermsOfUseFragment"
        android:label="TermsOfUseFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="de.gematik.ti.erp.app.settings.ui.SettingsFragment"
        android:label="SettingsFragment">
        <argument
            android:name="scrollTo"
            app:argType="de.gematik.ti.erp.app.settings.ui.SettingsScrollTo" />
        <action
            android:id="@+id/action_settingsFragment_to_debugFragment"
            app:destination="@id/debugFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/prescriptionDetailFragment"
        android:name="de.gematik.ti.erp.app.prescription.detail.ui.PrescriptionDetailFragment"
        android:label="PrescriptionDetailFragment">
        <action
            android:id="@+id/action_prescriptionDetailFragment_to_pharmacySearchFragment"
            app:destination="@id/pharmacySearchFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <argument
            android:name="taskId"
            app:argType="string" />
        <action
            android:id="@+id/action_prescriptionDetailFragment_to_cardWallFragment"
            app:destination="@id/cardWallFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/cardWallFragment"
        android:name="de.gematik.ti.erp.app.cardwall.ui.CardWallFragment"
        android:label="CardWallFragment" />
    <fragment
        android:id="@+id/onboardingFragment"
        android:name="de.gematik.ti.erp.app.onboarding.ui.OnboardingFragment"
        android:label="OnboardingFragment">
        <action
            android:id="@+id/action_onboardingFragment_to_mainScreenFragment"
            app:destination="@id/mainScreenFragment"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_onboardingFragment_to_dataTermsFragment"
            app:destination="@id/dataTermsFragment" />
        <action
            android:id="@+id/action_onboardingFragment_to_termsOfUseFragment"
            app:destination="@id/termsOfUseFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_userAuthenticationFragment"
        app:destination="@id/userAuthenticationFragment"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"
        app:popExitAnim="@anim/nav_default_exit_anim" />
    <action
        android:id="@+id/action_global_exit"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"
        app:popExitAnim="@anim/nav_default_exit_anim"
        app:popUpTo="@id/nav_graph"
        app:popUpToInclusive="true" />
    <fragment
        android:id="@+id/pharmacySearchFragment"
        android:name="de.gematik.ti.erp.app.pharmacy.ui.PharmacySearchFragment"
        android:label="PharmacySearchFragment">
        <argument
            android:name="taskIds"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/scanPrescriptionFragment"
        android:name="de.gematik.ti.erp.app.prescription.ui.ScanPrescriptionFragment"
        android:label="ScanPrescriptionFragment" />
    <fragment
        android:id="@+id/pickupCodeFragment"
        android:name="de.gematik.ti.erp.app.messages.ui.PickupCodeFragment"
        android:label="PickupCodeFragment">
        <argument
            android:name="pickupCodeHR"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="pickupCodeDMC"
            app:argType="string"
            app:nullable="true" />
    </fragment>
</navigation>
